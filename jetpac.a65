        sta     ZP_Addr_From+1          ; 3597: 85 07     Clear address hi-byte
        lda     ZP_Addr_From            ; 3599: A5 06     Addr_From = 71 00
        sec                             ; 359B: 38       
        sbc     #$40                    ; 359C: E9 40    
        sta     ZP_Addr_From            ; 359E: 85 06     Addr_From = 31 00, $31st Character ROM character is "1"

; Multiply Addr_From by 8, as 8 bytes per character definition
        ldx     #$03                    ; 35A0: A2 03    
CCTUR1  clc                             ; 35A2: 18       
        rol     ZP_Addr_From            ; 35A3: 26 06    
        rol     ZP_Addr_From+1          ; 35A5: 26 07     Addr_From = 31 00 > 62 00 > c4 00 > 88 01 i.e. $0188
        dex                             ; 35A7: CA        Left-shifted 3 times? i.e. multiplied by 8
        bne     CCTUR1                  ; 35A8: D0 F8     If no, loop back

; Add $8000 Character ROM offset to address
        lda     ZP_Addr_From+1          ; 35AA: A5 07    
        adc     #$80                    ; 35AC: 69 80    
        sta     ZP_Addr_From+1          ; 35AE: 85 07     Addr_From = 88 81 i.e. $8188 is Character ROM character "1"

; Copy 8 bytes to form a complete character
        ldy     #$07                    ; 35B0: A0 07    
        lda     ZP_INVERT               ; 35B2: A5 57     Test the Invert flag
        bmi     CCTUR_INV               ; 35B4: 30 15     If yes, copy bytes with inversion instead
CCTUR2  lda     (ZP_Addr_From),y        ; 35B6: B1 06     Copy bytes from given address with Y offset
        sta     (ZP_Addr_To),y          ; 35B8: 91 04     Copy bytes to given address with Y offset
        dey                             ; 35BA: 88        Decrement number of lines counter
        bpl     CCTUR2                  ; 35BB: 10 F9     All lines copied? If no, loop back.

; Move the next character address to the next column
CCTUR_NEXT
        lda     ZP_Addr_To              ; 35BD: A5 04    
        clc                             ; 35BF: 18       
        adc     #$B0                    ; 35C0: 69 B0     #B0=176=number of vertical pixels i.e. move to next char right
        sta     ZP_Addr_To              ; 35C2: 85 04    
        lda     ZP_Addr_To+1            ; 35C4: A5 05    
        adc     #$00                    ; 35C6: 69 00     Increment address hi-byte if previous addition overflowed
        sta     ZP_Addr_To+1            ; 35C8: 85 05    
        rts                             ; 35CA: 60       
; Copy 8 inverted bytes to form a complete character
CCTUR_INV
        lda     (ZP_Addr_From),y        ; 35CB: B1 06     Copy bytes from given address with Y offset
        eor     #$FF                    ; 35CD: 49 FF     Invert the byte (used to make the byte flash)
        sta     (ZP_Addr_To),y          ; 35CF: 91 04     Move bytes to given address with Y offset
        dey                             ; 35D1: 88        Decrement number of lines counter
        bpl     CCTUR_INV               ; 35D2: 10 F7     All lines copied? If no, loop back.
        jmp     CCTUR_NEXT              ; 35D4: 4C BD 35  If yes, routine ends
