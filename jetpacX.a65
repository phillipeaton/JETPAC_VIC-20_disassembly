    rol     a                               ; 33D7: 2A       
    rol     a                               ; 33D8: 2A        Leaves %FLDU_0000
    ora     #$8F                            ; 33D9: 09 8F     Set bits 7, 1-4, leaving %1LDU_1111
    sta     ZP1C_Jetman_Action              ; 33DB: 85 1C     Nothing pressed, byte=$FF at this point

