    lda     VIA1_Port_A                     ; 33DD: AD 11 91  Same as above
    ror     a                               ; 33E0: 6A       
    ror     a                               ; 33E1: 6A        Leaves %xxxx_FLDU
    ora     #$F7                            ; 33E2: 09 F7     Set bits 0-2 & 5-8, leaving %1111_F111
    and     ZP1C_Jetman_Action              ; 33E4: 25 1C     Merge result into previous reading and store
    sta     ZP1C_Jetman_Action              ; 33E6: 85 1C     Leaves %1LDU_F111

; Read direction Right
    lda     #$00                            ; 33E8: A9 00    
    sta     VIA2_DDR_B                      ; 33EA: 8D 22 91  Set Port B direction all lines read, probably
    lda     VIA2_Port_B                     ; 33ED: AD 20 91  %xxxx_Rxxx, reads $FF=%1111_1111 when nothing pressed
    ora     #$7F                            ; 33F0: 09 7F     Set bits 0-6, leaving %R111_1111
    and     ZP1C_Jetman_Action              ; 33F2: 25 1C     Merge result into previous reading and store
    sta     ZP1C_Jetman_Action              ; 33F4: 85 1C     Leaves %RLDU_F111
    rts                                     ; 33F6: 60       
